{
  "manifest_version": 3,
  "name": "Map My Udemy",
  "version": "1.0",
  "description": "Generates a mind map for Udemy courses",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.udemy.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
