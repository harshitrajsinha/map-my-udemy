{
  "manifest_version": 3,
  "name": "Map My Udemy",
  "version": "1.0",
  "description": "Generates a mind map for Udemy courses",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://www.udemy.com/*"],
  "action": {
    "default_icon": {
      "16": "images/image.png",
      "48": "images/image.png",
      "128": "images/image.png"
    },
    "default_title": "Map My Udemy",
    "default_popup": "popup.html"
  }
}
